services:
  db:
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: 1Secure*Password1
    image: mcr.microsoft.com/mssql/server:2017-latest
    ports:
    - published: 1433
      target: 1433
  mymessagequeue.api:
    build:
      context: C:\Users\mat\source\repos\MyMessageQueue
      dockerfile: MyMessageQueue.API/Dockerfile
    depends_on:
      db:
        condition: service_started
      rabbit1:
        condition: service_started
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: mymessagequeueapi
    ports:
    - target: 80
    - target: 443
    volumes:
    - C:\Users\mat\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\mat\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
  rabbit1:
    environment:
      RABBITMQ_DEFAULT_PASS: rabbitmq
      RABBITMQ_DEFAULT_USER: rabbitmq
      RABBITMQ_DEFAULT_VHOST: /
      RABBITMQ_ERLANG_COOKIE: SWQOKODSQALRPCLNMEQG
    hostname: rabbit1
    image: rabbitmq:3-management
    labels:
      NAME: rabbitmq1
    ports:
    - published: 5672
      target: 5672
    - published: 15672
      target: 15672
    volumes:
    - C:\Users\mat\source\repos\MyMessageQueue\enabled_plugins:/etc/rabbitmq/enabled_plugins:rw
version: '3.4'